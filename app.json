{
  "expo": {
    "name": "PlatuPro",
    "slug": "PlatuPro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fish_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/fish_logo_medium.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD0A3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wanruenik.PlatuPro",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fish_logo_medium.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wanruenik.PlatuPro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "d6c35d4a-8c43-4774-8669-2e230e275bd4"
      }
    }
  }
}
